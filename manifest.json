{
  "name": "Getting Started Example",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "/html/popup.html",
    "default_icon": {
      "16": "/images/ins.png",
      "32": "/images/ins.png",
      "48": "/images/ins.png",
      "128": "/images/ins.png"
    }
  },
  "devtools_page": "/html/devtools.html",
  "icons": {
    "16": "/images/ins.png",
    "32": "/images/ins.png",
    "48": "/images/ins.png",
    "128": "/images/ins.png"
  }
}